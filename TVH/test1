
#EXTM3U

#EXTINF:-1 tvg-name="005" tvg-id="E!" tvg-logo="https://raw.githubusercontent.com/matthuisman/iptv/master/nz/images/tv.51.png",E! UK New
pipe:///storage/.kodi/addons/tools.ffmpeg-tools/bin/ffmpeg -headers "referer:easyview.eu" -i "https://waw.easyview.eu/hls/e.m3u8" -f mpegts -vcodec copy -acodec copy pipe:1

#EXTINF:-1 ,Ball TV
pipe:///storage/.kodi/addons/tools.ffmpeg-tools/bin/ffmpeg -i https://stream.ecable.tv/kicc/index.m3u8 -y -vcodec libx264 -pix_fmt yuv420p -preset fast -crf 24 -vf scale="'w=if(gt(a,16/9),1920,-2):h=if(gt(a,16/9),-2,1080)'" -codec:a libfdk_aac -b:a 128k -movflags faststart -threads 4 output.mpegts pipe:1

